<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main.MainActivity">

    <Button
        android:id="@+id/issueabook"
        android:layout_width="202dp"
        android:layout_height="58dp"
        android:layout_marginStart="8dp"
        android:text="Issueabook"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="101dp" />

    <Button
        android:id="@+id/borrowabook"
        android:layout_width="121dp"
        android:layout_height="65dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="80dp"
        android:text="Button"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="182dp" />

    <Button
        android:id="@+id/profile"
        android:layout_width="102dp"
        android:layout_height="36dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="184dp"
        android:text="Button"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="260dp" />

    <Button
        android:id="@+id/addbook"
        android:layout_width="135dp"
        android:layout_height="75dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="175dp"
        android:text="ADD A BOOK"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="353dp" />

    <Button
        android:id="@+id/allbooks"
        android:layout_width="241dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="42dp"
        android:text="see all books" />

    <Button
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/allbooks"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="42dp"
        android:text="logout" />

    <Button
        android:id="@+id/requestbook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/borrowabook"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="106dp"
        android:text="request book" />

    <Button
        android:id="@+id/allrequests"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/addbook"
        android:layout_marginEnd="-23dp"
        android:layout_marginBottom="0dp"
        android:layout_toStartOf="@+id/logout"
        android:text="all requests" />

    <Button
        android:id="@+id/profileshow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/issueabook"
        android:layout_toEndOf="@+id/allbooks"
        android:text="profile" />

    <Button
        android:id="@+id/barcodescanner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/profile"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="108dp"
        android:text="barcode" />

    <Button
        android:id="@+id/Issuedbooks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="163dp"
        android:layout_toStartOf="@+id/logout"
        android:text="issuedbooks" />

    <Button
        android:id="@+id/changep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="99dp"
        android:layout_toEndOf="@+id/addbook"
        android:text="changep" />

    <Button
        android:id="@+id/mvpcheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/logout"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="17dp"
        android:layout_marginBottom="113dp"
        android:text="MVP CHECK" />

    <Button
        android:id="@+id/otpmobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="198dp"
        android:text="otpmobile" />

    <Button
        android:id="@+id/editprofile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/borrowabook"
        android:layout_alignEnd="@+id/mvpcheck"
        android:text="Editprofile" />

</RelativeLayout>