<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/namea"
        android:layout_width="97dp"
        android:layout_height="59dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="27dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/admia"
        android:layout_width="97dp"
        android:layout_height="58dp"
        android:layout_alignBottom="@+id/namea"
        android:layout_alignParentStart="true"
        android:layout_marginStart="103dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/pha"
        android:layout_width="77dp"
        android:layout_height="54dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="105dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/emaila"
        android:layout_width="91dp"
        android:layout_height="56dp"
        android:layout_alignTop="@+id/pha"
        android:layout_marginStart="-105dp"
        android:layout_toEndOf="@+id/pha"
        android:text="TextView" />

    <TextView
        android:id="@+id/booknamea"
        android:layout_width="90dp"
        android:layout_height="47dp"
        android:layout_alignEnd="@+id/namea"
        android:layout_alignParentTop="true"
        android:layout_marginTop="122dp"
        android:layout_marginEnd="-32dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/authora"
        android:layout_width="90dp"
        android:layout_height="54dp"
        android:layout_alignTop="@+id/booknamea"
        android:layout_centerHorizontal="true"
        android:text="TextView" />

    <TextView
        android:id="@+id/pricea"
        android:layout_width="91dp"
        android:layout_height="59dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="113dp"
        android:layout_marginEnd="40dp"
        android:text="TextView" />

</RelativeLayout>